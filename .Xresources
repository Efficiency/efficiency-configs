! font settings --------------------------------------------------------------

Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
!Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! --- special colors ---
*background: #000000
*foreground: #d8d8d8

! --- standard colors ---
! black
*color0: #030303
! red
*color1: #cc0000
! green
*color2: #4e9a06
! yellow
*color3: #c4a000
! blue
*color4: #3465a4
! magenta
*color5: #75507b
! cyan
*color6: #06989a
! white
*color7: #d3d7cf
! bright_black
*color8: #555753
! bright_red
*color9: #ef2929
! bright_green
*color10: #8ae234
! bright_yellow
*color11: #fce94f
! bright_blue
*color12: #729fcf
! bright_magenta
*color13: #ad7fa8
! bright_cyan
*color14: #34e2e2
! bright_white
*color15: #eeeeec

! Xft: Terminus fonts
!  Fedora/debian packages: terminus-fonts/xfonts-terminus
URxvt*font: xft:terminus:pixelsize=10
URxvt*boldFont: xft:terminus:bold:pixelsize=10

URxvt*scrollBar: false
! URxvt.scrollstyle: plain

URxvt.perl-ext-common: default,matcher,fullscreen,vtwheel

URxvt.url-launcher: x-www-browser
URxvt.matcher.button: 1
URxvt.colorUL: #9EB2C1

URxvt.tabbed.tabbar-fg: 7
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 0
URxvt.tabbed.tab-bg: 7

URxvt.keysym.F11: perl:fullscreen:switch

! run 'xrdb -load .Xresources' to update

